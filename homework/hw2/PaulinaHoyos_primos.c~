#include <stdio.h>
#include <stdlib.h>

void calculate(int num);



int main(int argc, char * argv[]){

 int n = sizeof(argv)/sizeof(char);

 if(argv[1][0] == '-'){printf("El número es negativo") }
else if(n>6){ printf("El número es de un orden mayor a 10e6"}
else {
    num = atoi(argv[1]);
    calculate(num);
 }

return 0;

}
}


